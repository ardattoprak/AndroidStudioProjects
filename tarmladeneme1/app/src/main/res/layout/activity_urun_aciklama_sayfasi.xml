        android:textSize="16sp"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="564dp" />

    <Button
        android:id="@+id/button6"
        style="@android:style/Widget.Material.Light.Button.Toggle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#479F4A"
        android:elevation="@dimen/cardview_default_elevation"
        android:text="See Seller Detail"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="636dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="363dp"
        android:layout_height="168dp"
        app:srcCompat="@mipmap/solanmumpatates"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="232dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="110dp"
        android:layout_height="96dp"
        app:srcCompat="@mipmap/tarimlalogoooooo"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="8dp" />

